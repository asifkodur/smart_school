CREATE TABLE DIV(ID INTEGER PRIMARY KEY   AUTOINCREMENT,YEAR  TEX NOT NULL,CLASS   TEXT NOT NULL,DIV TEXT NOT NULL);

CREATE TABLE USER (ID INTEGER PRIMARY KEY  AUTOINCREMENT, USER TEXT, PASSWORD TEXT);
CREATE TABLE "attendance" (
    "ID" INTEGER PRIMARY KEY   AUTOINCREMENT,"YEAR" TEXT  NOT NULL, "TERM" TEXT NOT NULL , "WORKING_DAYS" TEXT NOT NULL
);
CREATE TABLE ce_te ("ID" INTEGER PRIMARY KEY   AUTOINCREMENT, 
    "STD" TEXT,
    "SUBJECT" TEXT,
    "CE" INT,
    "TE" INT,
    "YEAR" TEXT
);
CREATE TABLE institution (
    "NAME" TEXT NOT NULL,
    "CODE" TEXT
, "DEO" TEXT, "EMAIL" TEXT, "CONTACT" TEXT);
CREATE TABLE working_days (
    "ID" INTEGER PRIMARY KEY   AUTOINCREMENT,
    "YEAR" TEXT,
    "TERM1" TEXT,
    "TERM2" TEXT,
    "TERM3" TEXT
);
CREATE TABLE BACKUP_TIME(ID INTEGER PRIMARY KEY   AUTOINCREMENT,YEAR TEX NOT NULL,MONTH TEXT NOT NULL,DAY TEXT NOT NULL, HOUR TEXT NOT NULL,MINUTE TEXT NOT NULL );
CREATE TABLE "STUDENT"(ID INTEGER PRIMARY KEY   AUTOINCREMENT,ADMISSION_NO  TEXT NOT NULL UNIQUE,NAME   TEXT  NOT NULL, 'UID' TEXT, 'GENDER' var1, 'DOB' text, 'CATEGORY' text, 'RELIGION' text,'CASTE' TEXT, 'FIRST_LANGUAGE' text, 'FATHER' text, 'MOTHER' text, 'PHONE' text, 'EMAIL' TEXT);
;
CREATE TABLE T1 (ID INTEGER PRIMARY KEY AUTOINCREMENT, STUDENT_ID INTEGER, DIV_ID INTEGER, ROLL NUMERIC, LANG_CE INT, LANG_TE INT, MAL_CE INT, MAL_TE INT,ENGLISH_CE INT,  ENGLISH_TE INT,  HINDI_CE INT, HINDI_TE INT, SS_CE INT,  SS_TE INT, PHYSICS_CE INT, PHYSICS_TE INT, CHEMISTRY_CE INT, CHEMISTRY_TE INT, BIOLOGY_CE INT, BIOLOGY_TE INT, MATHEMATICS_CE INT,  MATHEMATICS_TE INT, IT_CE INT, IT_TE INT, ATTENDANCE INT, MAX_ATTENDANCE INT, PROMOTED INTEGER);
CREATE TABLE T2 (ID INTEGER PRIMARY KEY AUTOINCREMENT, STUDENT_ID INTEGER, DIV_ID INTEGER, ROLL NUMERIC, LANG_CE INT, LANG_TE INT, MAL_CE INT, MAL_TE INT,ENGLISH_CE INT,  ENGLISH_TE INT,  HINDI_CE INT, HINDI_TE INT, SS_CE INT,  SS_TE INT, PHYSICS_CE INT, PHYSICS_TE INT, CHEMISTRY_CE INT, CHEMISTRY_TE INT, BIOLOGY_CE INT, BIOLOGY_TE INT, MATHEMATICS_CE INT,  MATHEMATICS_TE INT, IT_CE INT, IT_TE INT, ATTENDANCE INT, MAX_ATTENDANCE INT, PROMOTED INTEGER);
CREATE TABLE T3 (ID INTEGER PRIMARY KEY AUTOINCREMENT, STUDENT_ID INTEGER, DIV_ID INTEGER, ROLL NUMERIC, LANG_CE INT, LANG_TE INT, MAL_CE INT, MAL_TE INT,ENGLISH_CE INT,  ENGLISH_TE INT,  HINDI_CE INT, HINDI_TE INT, SS_CE INT,  SS_TE INT, PHYSICS_CE INT, PHYSICS_TE INT, CHEMISTRY_CE INT, CHEMISTRY_TE INT, BIOLOGY_CE INT, BIOLOGY_TE INT, MATHEMATICS_CE INT,  MATHEMATICS_TE INT, IT_CE INT, IT_TE INT, ATTENDANCE INT, MAX_ATTENDANCE INT, PROMOTED INTEGER);
